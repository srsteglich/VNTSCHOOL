Algoritmo "Fatorial"

Var
   num, i, fator: inteiro


Inicio

   escreva("Digite um número: ")
   leia(num)
   escreval("------------------------------------")
   fator <- 1
   para i de 1 ate num faca
        fator <- fator * i
   fimpara
   escreval("O Fatorial de ",num, " é ",fator)

Fimalgoritmo