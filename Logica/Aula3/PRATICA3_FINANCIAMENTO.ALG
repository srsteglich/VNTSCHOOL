Algoritmo "Financiamento"

Var
   sal: real


Inicio
      escreva("Digite salario: ")
      leia(sal)
            se (sal >=5) entao
               escreva("O ", sal, " salario, então  Financiamento Concedido")
            senao
               escreva("O ", sal, " salario, então  Financiamento Negado")
      fimse
Fimalgoritmo